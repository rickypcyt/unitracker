{
  "name": "vitejs-uni-tracker",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"bun vite\"",
    "build": "vite build",
    "build:mobile": "DISABLE_COMPRESSION=true vite build",
    "build-only": "vite build",
    "vite": "vite",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:strict": "eslint . --ext .ts,.tsx,.js,.jsx --max-warnings 0",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "prepare": "husky install",
    "precommit": "lint-staged",
    "cap:init": "cap init \"UniTracker\" com.example.unitracker --web-dir=dist --npm-client npm",
    "cap:sync": "cap sync",
    "ios:add": "cap add ios",
    "ios:sync": "cap sync ios",
    "ios:run": "cap run ios",
    "android:build": "bun run build:mobile && npx cap sync android && cd android && ./gradlew assembleDebug",
    "android:install": "adb install -r android/app/build/outputs/apk/debug/app-debug.apk",
    "android:update": "bun run build:mobile && npx cap sync android && cd android && ./gradlew assembleDebug && adb install -r app/build/outputs/apk/debug/app-debug.apk"
  },
