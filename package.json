{
  "name": "vitejs-uni-tracker",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"bun vite\"",
    "build": "vite build",
    "build:fast": "vite build --mode development",
    "build:mobile": "DISABLE_COMPRESSION=true vite build",
    "build:mobile:fast": "DISABLE_COMPRESSION=true vite build --mode development",
    "build:analyze": "ANALYZE=true vite build",
    "build-only": "vite build",
    "vite": "vite",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:strict": "eslint . --ext .ts,.tsx,.js,.jsx --max-warnings 0",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "prepare": "husky install",
    "precommit": "lint-staged",
    "cap:init": "cap init \"UniTracker\" com.example.unitracker --web-dir=dist --npm-client npm",
    "cap:sync": "cap sync",
    "ios:add": "cap add ios",
    "ios:sync": "cap sync ios",
    "ios:run": "cap run ios",
    "android:build": "bun run build:mobile && npx cap sync android && cd android && ./gradlew assembleDebug",
    "android:install": "adb install -r android/app/build/outputs/apk/debug/app-debug.apk",
    "android:update": "bun run build:mobile && npx cap sync android && cd android && ./gradlew assembleDebug && adb install -r app/build/outputs/apk/debug/app-debug.apk"
  },
  "dependencies": {
    "@capacitor/android": "^7",
    "@capacitor/core": "^7",
    "@capacitor/status-bar": "^7.0.3",
    "@chakra-ui/react": "^3.24.2",
    "@dayflow/core": "^2.0.4",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@eslint/eslintrc": "^3.3.1",
    "@heroicons/react": "^2.2.0",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/react-select": "^2.2.5",
    "@reduxjs/toolkit": "^2.11.0",
    "@supabase/supabase-js": "^2.54.0",
    "@tiptap/extension-placeholder": "^3.14.0",
    "@tiptap/pm": "^3.0.9",
    "@tiptap/react": "^3.0.9",
    "@tiptap/starter-kit": "^3.0.9",
    "@vercel/analytics": "^1.5.0",
    "@vercel/speed-insights": "^1.2.0",
    "@vitejs/plugin-react": "^5.1.2",
    "chart.js": "^4.5.1",
    "clsx": "^2.1.1",
    "concurrently": "^9.2.0",
    "date-fns": "^4.1.0",
    "eslint-plugin-security": "^3.0.1",
    "eslint-plugin-tailwindcss": "^3.18.2",
    "framer-motion": "^12.23.12",
    "highlight.js": "^11.11.1",
    "lucide-react": "^0.563.0",
    "moment": "^2.30.1",
    "react": "^18.3.1",
    "react-datepicker": "^8.4.0",
    "react-dom": "^18.3.1",
    "react-helmet-async": "^2.0.5",
    "react-hot-toast": "^2.5.2",
    "react-icons": "^5.5.0",
    "react-markdown": "^10.1.0",
    "react-redux": "^9.2.0",
    "react-router-dom": "^7.8.0",
    "react-slider": "^2.0.6",
    "react-toastify": "^11.0.5",
    "recharts": "^2.15.4",
    "redux": "^5.0.1",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7",
    "terser": "^5.43.1",
    "tone": "^15.1.22",
    "vite-plugin-compression2": "^1.4.0",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@capacitor/cli": "^7",
    "@sentry/react": "^7.120.4",
    "@sentry/tracing": "^7.120.4",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/nesting": "^0.0.0-insiders.565cd3e",
    "@types/node": "^22.17.0",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react-swc": "^3.11.0",
    "autoprefixer": "^10.4.24",
    "baseline-browser-mapping": "^2.9.19",
    "eslint": "^9.32.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.4",
    "postcss": "^8.5.6",
    "postcss-nesting": "^14.0.0",
    "rollup-plugin-visualizer": "^6.0.5",
    "tailwindcss": "3.4.0",
    "vite": "^4.5.14"
  },
  "peerDependencies": {
    "typescript": "^5.9.2"
  },
  "trustedDependencies": [
    "@swc/core",
    "@vercel/speed-insights",
    "core-js",
    "esbuild"
  ]
}
