# Fastlane Appfile for iOS metadata
# You can override these via environment variables in CI

app_identifier(ENV['BUNDLE_ID'] || 'com.example.unitracker')
apple_id(ENV['APPLE_ID'] || 'your-apple-id@example.com')

itc_team_id(ENV['APP_STORE_TEAM_ID']) # App Store Connect Team ID (optional)
team_id(ENV['APPLE_TEAM_ID'])         # Developer Portal Team ID (optional)

# The API key is provided via appstore_api_key.json in the Fastfile lane
